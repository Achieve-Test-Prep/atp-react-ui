{
  "name": "atp-react-ui",
  "description": "A new default theme for ATP Products.",
  "private": false,
  "version": "5.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css",
    "./config": "./config.js"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "config.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui/issues"
  },
  "homepage": "https://github.com/Achieve-Test-Prep/atp-react-ui#readme",
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "predev": "tailwindcss -i style/tailwind.css -o style/output.css -c style/tailwind.config.js --watch",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cz": "git-cz",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@headlessui/react": ">=1.7.13",
    "@heroicons/react": "^2.0.16",
    "@radix-ui/react-accordion": ">=1.1.2",
    "@radix-ui/react-label": ">=2.0.2",
    "@radix-ui/react-progress": ">=1.0.3",
    "@radix-ui/react-radio-group": ">=1.1.3",
    "@radix-ui/react-slot": ">=1.0.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tippyjs/react": "^4.2.6",
    "react": ">=18.3.0",
    "react-dom": "18.3.0",
    "react-hook-form": ">=7.44.2",
    "tailwind-merge": ">=1.10.0",
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.0.16",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@release-it/conventional-changelog": "^8.0.2",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tippyjs/react": "^4.2.6",
    "@types/node": "^20.17.23",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.44.2",
    "release-it": "^17.11.0",
    "storybook": "^8.6.4",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-slider": "^1.2.3"
  }
}