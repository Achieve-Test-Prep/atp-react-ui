{
  "name": "atp-react-ui",
  "version": "2.6.0",
  "description": "A new default theme for ATP Products.",
  "main": "dist/index.js",
  "files": [
    "lib",
    "dist",
    "config.js",
    "README.md"
  ],
  "scripts": {
    "predev": "tailwindcss -i style/tailwind.css -o style/output.css -c style/tailwind.config.js --watch",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "codecov": "codecov",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:ts": "tsc --declaration --declarationMap --emitDeclarationOnly",
    "build:lib": "babel src/themes/default.ts --out-file lib/defaultTheme.js",
    "prebuild": "rimraf dist lib && npm run build:lib && npm run build:ts",
    "build:tailwind": "tailwindcss -i style/tailwind.css -o style/output.css -c style/tailwind.config.js --minify",
    "build": "webpack",
    "prestorybook": "npm run build:tailwind",
    "build-storybook": "build-storybook",
    "cz": "git-cz",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.13",
    "@tippyjs/react": "^4.2.6",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@storybook/react": "^6.5.15",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tippyjs/react": "^4.2.6",
    "deepmerge": "4.2.2",
    "postcss": "^8.4.21",
    "react-focus-lock": "2.9.2",
    "react-transition-group": "4.4.5",
    "tailwind-merge": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@release-it/conventional-changelog": "^5.1.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@svgr/webpack": "^6.5.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-transition-group": "4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@zarconontol/enzyme-adapter-react-18": "0.7.3",
    "babel-jest": "^29.4.0",
    "babel-loader": "^9.1.2",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "release-it": "^15.6.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui/issues"
  },
  "homepage": "https://github.com/Achieve-Test-Prep/atp-react-ui#readme",
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".*\\.d\\.ts",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/{Transition,index}.{ts,tsx}",
      "!src/{stories,utils}/*"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transform": {
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
