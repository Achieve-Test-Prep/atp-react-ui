{
  "name": "atp-react-ui",
  "description": "A new default theme for ATP Products.",
  "private": false,
  "version": "4.0.9",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style/atp-css-styles.css": "./style/atp-css-styles.css",
    "./style/fonts.css": "./style/fonts.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "style",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui/issues"
  },
  "homepage": "https://github.com/Achieve-Test-Prep/atp-react-ui#readme",
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./style/tailwind.css -o ./style/atp-css-styles.css --minify",
    "watch:css": "npx @tailwindcss/cli -i ./style/tailwind.css -o ./style/atp-css-styles.css --watch",
    "build": "npm run build:css && tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "storybook": "npm run build:css && storybook dev -p 6006",
    "build-storybook": "npm run build:css && storybook build",
    "cz": "git-cz",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@headlessui/react": ">=2.2.7",
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "react-hook-form": ">=7.61.1",
    "tailwind-merge": ">=3.3.1",
    "tailwindcss": ">=4.1.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.21.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tippyjs/react": "^4.2.6",
    "@types/node": "^20.19.10",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "release-it": "^19.0.4",
    "storybook": "^9.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slider": "^1.3.5",
    "@tailwindcss/forms": "^0.5.10"
  }
}
