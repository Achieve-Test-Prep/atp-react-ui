{
  "name": "atp-react-ui",
  "description": "A new default theme for ATP Products.",
  "private": false,
  "version": "5.2.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style/atp-css-styles.css": "./dist/index.css"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Achieve-Test-Prep/atp-react-ui/issues"
  },
  "homepage": "https://github.com/Achieve-Test-Prep/atp-react-ui#readme",
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./style/tailwind.css -o ./style/atp-css-styles.css",
    "watch:css": "npx @tailwindcss/cli -i ./style/tailwind.css -o ./style/atp-css-styles.css --watch",
    "build": "npm run build:css && tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "storybook": "npm run build:css && storybook dev -p 6006",
    "build-storybook": "npm run build:css && storybook build",
    "cz": "git-cz",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@headlessui/react": ">=2.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": ">=7.54.2",
    "tailwind-merge": ">=3.0.2",
    "tailwindcss": ">=4.0.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.21.0",
    "@headlessui/react": "^2.2.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@release-it/conventional-changelog": "^10.0.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.3",
    "@tippyjs/react": "^4.2.6",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "release-it": "^18.1.2",
    "storybook": "^8.6.12",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-slider": "^1.2.4"
  }
}
